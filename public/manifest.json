{
  "manifest_version": 3,
  "name": "Dumb Tube",
  "version": "1.0",
  "description": "A Chrome extension built with React and TypeScript using Vite",
  "permissions": ["activeTab","scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
